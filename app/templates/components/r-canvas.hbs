<canvas width={{width}} height={{height}}></canvas>
<div class="controls">
    <div class="row">
        <div class="col s6">x1: {{input value=x1}}</div>
        <div class="col s6">y1: {{input value=y1}}</div>
    </div>
    <div class="row">
        <div class="col s6">x2: {{input value=x2}}</div>
        <div class="col s6">y2: {{input value=y2}}</div>
    </div>
    <div class="row">
        <div class="col s3">{{input value=endTime}}</div>
        <div class="col s9">{{md-range value=endTime step="3600" min="1490918400" max="1491238800" name="End time (UTC seconds)"}}</div>
        <div class="col s12">{{moment-format (mult 1000 endTime)}} ({{endTime}})</div>
    </div>
</div>
